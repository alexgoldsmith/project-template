{
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
    "trailingComma": "es5",
    "useTabs": true,
    "overrides": [
        {
            "files": [
                ".*",
                "*.js",
                "*.tsx",	
                "*.json",
                "*.yml"
            ],
            "options": {
                "tabWidth": 2,
                "useTabs": false
            }
        }
    ]
}